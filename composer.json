{
  "name": "laravel/cognito-auth",
  "description": "An authentication driver for Laravel for authenticating users in AWS Cognito User Pools",
  "keywords": ["php", "laravel", "auth", "aws", "cognito"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ahmed Gad",
      "email": "ahmed.ali-gad@outlook.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "aws/aws-sdk-php": "^3.190",
    "nesbot/carbon": "^2.5",
    "laravel/framework": "^8.40"
  },
  "autoload": {
    "psr-0": {
      "Pallant\\LaravelAwsCognitoAuth": "src/"
    }
  },
  "extra": {
    "laravel": {
        "providers": [
            "Pallant\\LaravelAwsCognitoAuth\\ServiceProvider"
        ]
    }
  }
}
